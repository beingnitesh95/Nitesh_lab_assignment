package com.bankapp.dto;

import lombok.Data;
import lombok.Getter;
import lombok.NoArgsConstructor;
import lombok.Setter;

@Data
@NoArgsConstructor
@Getter
@Setter
public class AccountDto {

    private String name;
    private String email;
    private int balance;

    public AccountDto(String name, String email, int balance) {
        this.name = name;
        this.email = email;
        this.balance = balance;
    }
}
